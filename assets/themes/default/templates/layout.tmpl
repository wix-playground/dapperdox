      [: else :]

        <div class="col-xs-3 col-sm-3 col-md-3 col-lg-3"> 
            [: template "fragments/sidenav" . :] 
        </div>

        <div class="col-xs-9 col-sm-9 col-md-7 col-lg-7 main"> 
      [: end :]

          [: yield :]
        </div>

